generator client {
    provider        = "prisma-client-js"
    previewFeatures = ["referentialIntegrity"]
}

datasource db {
    provider             = "mysql"
    url                  = env("DATABASE_URL")
    referentialIntegrity = "prisma"
}

model Cache {
    id       String    @unique
    contents String    @db.LongText
    cachedAt DateTime? @default(now())
}
