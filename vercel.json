{
    "cleanUrls": true,
    "trailingSlash": false,
    "rewrites": [
        { "source": "/", "destination": "/api" },
        { "source": "/videos", "destination": "/api/videos" },
        { "source": "/github", "destination": "/api/github" },
        { "source": "/twitter", "destination": "/api/twitter" },
        { "source": "/(.*)", "destination": "/api/404" }
    ],
    "redirects": [
        { "source": "(.*).(.*)", "destination": "/404"}
    ]
}